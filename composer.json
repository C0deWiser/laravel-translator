{
    "name": "codewiser/polyglot",
    "description": "Localize Laravel applications",
    "keywords": ["gettext", "poedit", "trans", "Laravel"],
    "license": "MIT",
    "version": "0.1.0",
    "authors": [
        {
            "name": "pm",
            "email": "pm@101media.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-gettext": "*",
        "ext-json": "*",
        "laravel/framework": "^8.0",
        "sepia/po-parser": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Codewiser\\Polyglot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codewiser\\Polyglot\\PolyglotServiceProvider"
            ]
        }
    }
}
